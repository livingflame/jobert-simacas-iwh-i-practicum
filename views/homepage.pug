doctype html
html 
    head 
        title= `${title}`
        meta(name="viewport" content="width=device-width, initial-scale=1")
        link(rel="stylesheet", href="/css/style.css")
    body    
        h1 #{title}
        a(href="http://localhost:3000/update-cobj/") Add to this table
        table(style='width:100%', border='1')
            tr
                th ID
                th Project
                th Completion
                th Edit
            each project in data
                tr
                    td #{project.properties.project_id}
                    td #{project.properties.project_name}
                    td #{project.properties.project_page}
                    td
                        a(href=`http://localhost:3000/update-cobj/?id=${project.properties.hs_object_id}`) Edit